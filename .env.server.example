# 服务器生产环境配置示例
# 复制此文件为 .env 用于服务器部署

# Docker 部署时使用容器名称
DB_HOST=postgres
DB_PORT=5432
DB_NAME=ocr_db
DB_USER=ocr_user
DB_PASSWORD=change_me_in_production

# Redis 配置
REDIS_HOST=redis
REDIS_PORT=6379

# 生产环境必须修改的配置
AUTH_SECRET_KEY=PLEASE_CHANGE_THIS_TO_RANDOM_STRING

# LLM API 配置
PRIMARY_LLM_PROVIDER=gemini
AISTUDIO_API_KEY=your_api_key_here
