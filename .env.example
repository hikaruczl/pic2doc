# OpenAI API Configuration
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4-vision-preview
OPENAI_MAX_TOKENS=4096

# Anthropic Claude API Configuration
ANTHROPIC_API_KEY=your_anthropic_api_key_here
ANTHROPIC_MODEL=claude-3-opus-20240229

# Google Gemini API Configuration
AISTUDIO_API_KEY=your_gemini_api_key_here
# GEMINI_API_KEY=
GEMINI_MODEL=gemini-1.5-flash
GEMINI_MAX_TOKENS=4096

# Alibaba Qwen-VL API Configuration
QWEN_API_KEY=your_qwen_api_key_here
QWEN_MODEL=qwen-vl-plus

# Primary LLM Provider (openai, anthropic, gemini, or qwen)
PRIMARY_LLM_PROVIDER=gemini

# Fallback LLM Provider (openai, anthropic, gemini, qwen, or none)
FALLBACK_LLM_PROVIDER=qwen

# API Settings
API_TIMEOUT=60
API_MAX_RETRIES=3
API_RETRY_DELAY=2

# Image Processing Settings
MAX_IMAGE_SIZE_MB=10
SUPPORTED_IMAGE_FORMATS=png,jpg,jpeg,pdf
IMAGE_QUALITY=95

# Output Settings
OUTPUT_DIR=output
LOG_DIR=logs
LOG_LEVEL=INFO

# Document Settings
DEFAULT_FONT=Arial
DEFAULT_FONT_SIZE=11
INCLUDE_ORIGINAL_IMAGE=true

